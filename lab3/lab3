#include <stdio.h>
int i = 0;
int my_strlen(char* s){
    int i = 0;
    while (*s++)i++;
    return i;
}

int rec_strlen(char* s){
    if (*s==0) {return 0;}
    else {return rec_strlen(s+1)+1;};
}

int main() {
    char s[] = "UI is a girl!";
    printf("len = %d\n", my_strlen(s));
    printf("len = %d\n", rec_strlen(s));
    return 0;
}
